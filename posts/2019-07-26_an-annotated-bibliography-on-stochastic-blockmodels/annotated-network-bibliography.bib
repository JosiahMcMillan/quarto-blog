	language = {en},
	number = {11},
	urldate = {2019-03-07},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Chung, F. and Lu, L. and Vu, V.},
	month = may,
	year = {2003},
	pages = {6313--6318},
	file = {Chung et al. - 2003 - Spectra of random graphs with given expected degre.pdf:C\:\\Users\\alex\\Zotero\\storage\\QDVL5ZJL\\Chung et al. - 2003 - Spectra of random graphs with given expected degre.pdf:application/pdf}
