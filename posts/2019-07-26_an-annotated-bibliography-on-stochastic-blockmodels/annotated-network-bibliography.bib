	keywords = {Mathematics - Statistics Theory, Statistics - Machine Learning},
	pages = {1878--1915},
	annote = {Comment: Published in at http://dx.doi.org/10.1214/11-AOS887 the Annals of Statistics (http://www.imstat.org/aos/) by the Institute of Mathematical Statistics (http://www.imstat.org)},
	file = {Rohe et al. - 2011 - Spectral clustering and the high-dimensional stoch.pdf:C\:\\Users\\alex\\Zotero\\storage\\YS8HTZ4S\\Rohe et al. - 2011 - Spectral clustering and the high-dimensional stoch.pdf:application/pdf}
